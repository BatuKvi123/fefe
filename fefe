--MW_animatorProgressSave: RightLeg,1,0,0,1,-120,30,3,-2,-1,0,0,1,80,10,,-2,-0,0,1,2,90,0,0,1,RightArm,0.75,0,0,1,-10,0,0,1,-0.3,0,3,-2,160,0,1,2,-0.25,0,0,1,90,5,,-2,Head,0,0,0,1,-130,20,2,-2,1,0,0,1,0,0,0,1,-0.1,0,0,1,200,5,1,-2,PrimeRaven_Handle,8.658513905857035e-09,0,0,1,0,0,0,1,0.7000002861022949,0,0,1,0,0,0,1,-0.050000011920928955,0,0,1,0,0,0,1,LeftLeg,-1,0,0,1,-120,30,3,-2,-1,0.25,5,2,-100,10,1,2,-0.75,0.25,1,2,-90,0,0,1,LeftArm,-0.75,0,0,1,-10,0,0,1,0,,0,-2,-170,0,0,1,-0.25,0,0,1,-100,10,0,2,Torso,0,0.1,2,-2,-60,-20,2,-2,5,0.5,1,-
    end
})
end


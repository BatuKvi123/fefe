function gameE()
local mode = "GAME"
addmode("v", {
    idle = function()
    --AccessoryWeld.C0 = AccessoryWeld.C0:Lerp(cf(-0.2021636962890625, -0.12768173217773438, -0.2653465270996094) * angles(0, 0, 0), deltaTime) 
